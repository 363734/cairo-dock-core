msgstr "在指向的图标上"

#: ../data/messages:825
msgid "On all icons"
msgstr "在所有图标上"

#: ../data/messages:827
msgid "Font"
msgstr "字体"

#: ../data/messages:831
msgid "Use a custom font for labels?"
msgstr "标签中使用自定义字体吗?"

#: ../data/messages:833
msgid "Font used for labels :"
msgstr "标签的字型："

#: ../data/messages:837
msgid "Colour"
msgstr "颜色"

#: ../data/messages:839
msgid "It's the first color of the gradation."
msgstr "这是渐变的第一个颜色。"

#: ../data/messages:841
msgid "Start color :"
msgstr "起始颜色："

