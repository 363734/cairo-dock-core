msgid "Image's transparency :"
msgstr "Прозорість зображення:"

#: ../data/messages:301 ../data/messages:353 ../data/messages:455
#: ../data/messages:579 ../data/messages:605
msgid "transparent"
msgstr "прозорість"

#: ../data/messages:303 ../data/messages:355 ../data/messages:457
#: ../data/messages:581 ../data/messages:607
msgid "opaque"
msgstr "щільність"

