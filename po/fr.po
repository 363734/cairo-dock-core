msgstr "Durée de l'animation de déploiement :"

#: ../data/messages:315 ../data/messages:323 ../data/messages:327
#: ../data/messages:335 ../data/messages:343 ../data/messages:347
#: ../data/messages:511
msgid "fast"
msgstr "Rapide"

